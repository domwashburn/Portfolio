{
  "name": "dom-washburns-portfolio",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build:dev": "better-npm-run build:dev",
    "build:gh-pages": "better-npm-run build:gh-pages",
    "build:dist": "better-npm-run build:dist",
    "dev": "npm-run-all --parallel build:dev lint:watch",
    "pages": "better-npm-run build:gh-pages",
    "dist": "better-npm-run build:dist",
    "lint": "node_modules/.bin/esw webpack.config.js src tools",
    "lint:watch": "npm run lint -- --watch",
    "pregh-pages": "gulp build; git add .build; git commit -m 'updating gh-pages'; git push origin dev",
    "gh-pages": "git subtree push --prefix .build origin gh-pages",
    "serve": "webpack-dev-server",
    "test": "jest --coverage",
    "test:watch": "npm run test -- --watch"
  },
  "betterScripts": {
    "build:dev": {
      "command": "webpack-dev-server --config webpack.config.js --progress --env.sourcemap",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:dist": {
      "command": "rimraf dist; webpack --progress --env.extractText --env.sourcemap",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:gh-pages": {
      "command": "rimraf gh-pages; webpack --progress --env.extractText --env.sourcemap; babel-node tools/server.js",
      "env": {
        "NODE_ENV": "gh-pages"
      }
    }
  },
  "author": "Dominick Washburn <domwashburn@gmail.com> (http://domwashburn.com)",
  "license": "ISC",
  "dependencies": {
    "better-npm-run": "0.0.14",
    "npm-run-all": "^4.0.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-typekit": "^1.0.6"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-react": "^1.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-es2015-native-modules": "^6.9.4",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-1": "^6.22.0",
    "babel-register": "^6.24.0",
    "classnames": "^2.2.5",
    "colors": "^1.1.2",
    "commitizen": "^2.9.6",
    "compression": "^1.6.2",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.27.3",
    "cssnano": "^3.10.0",
    "cz-conventional-changelog": "^2.0.0",
    "enzyme": "^2.7.1",
    "eslint": "^3.17.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.10.0",
    "eslint-watch": "^3.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "html-webpack-harddisk-plugin": "^0.1.0",
    "html-webpack-plugin": "^2.28.0",
    "ibm-design-colors": "^2.0.3",
    "istanbul": "^0.4.5",
    "jest": "^19.0.2",
    "jest-cli": "^19.0.2",
    "node-sass": "^4.5.0",
    "open": "0.0.5",
    "path": "^0.12.7",
    "postcss-import": "^9.1.0",
    "postcss-load-config": "^1.2.0",
    "postcss-loader": "^1.3.3",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.4.2",
    "resolve-url-loader": "^2.0.2",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.14.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-hot-middleware": "^2.17.1"
  }
}
